<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.twitterapi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Twitter_API_app">
        <activity android:name=".Mail"></activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".Search" />
        <activity android:name=".Edited" />
        <activity android:name=".Details" />
        <activity android:name=".ViewAll" />
        <activity android:name=".Delete" />
        <activity android:name=".Edit" />
        <activity android:name=".Create" />
        <activity android:name=".Manage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>